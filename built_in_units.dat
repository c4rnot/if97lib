,{ "R", "째R", TEMPERATURE,  { { 0.0, 5.0/9.0 }, 0.0}, "K", false}
,{ "F", "째F", TEMPERATURE, { {(+ 459.67 * 5.0/9.0), 5.0/9.0 }, 0.0}, "K", false}


